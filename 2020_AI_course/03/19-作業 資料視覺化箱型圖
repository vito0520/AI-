import numpy as np
import matplotlib.pyplot as plt

deta = [178 ,  164,  159,  162,  182,
             179   ,166  ,168  ,173  ,165]


newDeta = sorted(deta)

mAX = np.max(deta)
mIn = np.min((deta))
med = np.median(deta)




fig1, ax1 = plt.subplots()
ax1.set_title('Basic Plot')
ax1.boxplot(newDeta)
plt.show()
